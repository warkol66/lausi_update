<?xml version="1.0" encoding="utf8"?>
<config>
	<system>
		<parameters>
			<debugMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</debugMode>
			<developmentMode>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</developmentMode>
			<mailCapture>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</mailCapture>
			<underMaintenance>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</underMaintenance>
			<maintenanceMessage>El sistema permanecerá en mantenimiento por 2 horas, por favor regrese mas tarde.</maintenanceMessage>
			<affiliateUserLoginUnified>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</affiliateUserLoginUnified>
			<applicationTimeZoneGMT>
				<value>-3</value>
				<element_type>TIMEZONE</element_type>
			</applicationTimeZoneGMT>
			<dateFormat>
				<value>d-m-Y</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y</option_1>
					<option_2>m-d-Y</option_2>
					<option_3>d-m-y</option_3>
					<option_4>m-d-y</option_4>
				</element_options>
			</dateFormat>
			<dateTimeFormat>
				<value>d-m-Y H:M:S</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y H:M:S</option_1>
					<option_2>d-m-Y I:M:S P</option_2>
					<option_3>H:M:S d-m-Y</option_3>
					<option_4>I:M:S P d-m-Y</option_4>
				</element_options>
			</dateTimeFormat>
			<thousandsSeparator>.</thousandsSeparator>
			<decimalSeparator>,</decimalSeparator>
			<numberOfDecimals>2</numberOfDecimals>
			<siteName>LAUSI - Vía Pública</siteName>
			<siteDescription>Sistema de Gestión de Activos de Lausi Vía Pública</siteDescription>
			<siteShortName>lausiBiz</siteShortName>
			<webmasterMail>moddebugger@gmail.com</webmasterMail>
			<debugMail>moddebugger@gmail.com</debugMail>
			<fromEmail>depositolausi@fibercorp.com.ar</fromEmail>
			<tablePrefix></tablePrefix>
			<news>Novedades: Bienvenido al Sistema de Gestión de Activos Publicitarios de Lausi - Vía Pública.</news>
			<affiliateNews>Novedades: Bienvenido al Sistema de Gestión de Activos de Lausi - Vía Pública.</affiliateNews>
		</parameters>
		<errorReporting>
			<value>NONE</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>NONE</option_1>
				<option_2>E_ALL ^ E_NOTICE</option_2>
				<option_3>E_ALL</option_3>
				<option_4>E_STRICT</option_4>
			</element_options>
		</errorReporting>
		<rowsPerPage>50</rowsPerPage>
		<receiveMailBackup>depositolausi@fibercorp.com.ar,moddebugger@gmail.com</receiveMailBackup>
	</system>
	<lausi>
		<availableBillboardsDays>5</availableBillboardsDays>
	</lausi>
	<users>
		<licenses>5</licenses>
	</users>
	<email>
		<mode>
			<value>SMTP</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>PHP</option_1>
				<option_2>SMTP</option_2>
			</element_options>
		</mode>
		<smtpOptions>
			<smtpHost>lausiupdate.sistemas.biz</smtpHost>
			<smtpPort>587</smtpPort>
			<smtpUsername>debugger@lausiupdate.sistemas.biz</smtpUsername>
			<smtpPassword>Debug2k13</smtpPassword>
			<smtpSsl>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</smtpSsl>
		</smtpOptions>
	</email>
</config>
